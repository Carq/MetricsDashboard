{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Cors": {
    "Name": "tilesCors",
    "AllowedOrigins": [ "http://localhost:8080" ],
    "AllowedHeaders": [
      "Origin",
      "X-Requested-With",
      "Content-Type",
      "Response-Type",
      "Accept",
      "authorization",
      "request-id",
      "user-agent"
    ],
    "AllowedMethods": [ "POST", "GET", "OPTIONS", "PUT", "DELETE" ],
    "AllowCredentials": true
  },
  "Application": {
    "SecurityToken": "secret"
  }
}
